<% @page_title = "UserAuth | Login" -%>
<div class= "Sign_Form">
	<h1>Log in</h1>
	<%= form_tag(:action => 'login_attempt') do %>
		<p>
		Name:</br> <%= text_field_tag(:username) %>
		</p> 

		<p>
		LastName:</br> <%= text_field_tag(:userlastname) %>
		</p> 

		<p>
		Password:</br> <%= password_field_tag(:login_password) %>
		</p> 

		<%= submit_tag("Log In") %>
	<% end %>
</div>